
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">

    <div class="row">
        <div class="col-md-3">
            <br />
            <!--Filtreleme Kısmı-->
            <form class="form-horizontal">
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label"> Least</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="Least(Price)">
                    </div>
                </div>
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label">Most</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="Most(Prize)">
                    </div>
                </div>
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label">City</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="City">
                    </div>
                </div>
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label">District</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="District">
                    </div>
                </div>
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label">Neighborhood</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="Neighborhood">
                    </div>
                </div>
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label">Situation</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="Situation">
                    </div>
                </div>
                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label">Type</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" placeholder="Type">
                    </div>
                </div>



                <div class="form-group ">
                    <label for="inputEmail3" class="col-sm-4 control-label"></label>
                    <div class="col-sm-6">
                        <button type="submit" class="btn btn-primary btn-block"><i class="fa fa-search" aria-hidden="true"></i> FILTER</button>
                    </div>
                </div>
            </form>
        </div>

        <!--ilan Listesi-->
        <br />
        <div class="col-md-9">
            <div class="thumbnail" style="width:845px; height:400px">

                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/Admin/Index">Admin Sayfası</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Anasayfa</li>
                    </ol>
                </nav>
                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("City List", "Index", "Sehir", null, new { @class = "btn btn-success btn-block" })</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("District List", "Index", "Semt", null, new { @class = "btn btn-info btn-block" })</p>
                        </div>
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("Neighborhood List", "Index", "Mahalle", null, new { @class = "btn btn-primary btn-block" })</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("Situation List", "Index", "Durum", null, new { @class = "btn btn-warning btn-block" })</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("Type List", "Index", "Tip", null, new { @class = "btn btn-success btn-block" })</p>
                        </div>
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("Advertise List", "IlanListesi", "Admin", null, new { @class = "btn btn-info btn-block" })</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="~/Content/images/analiz.jpg" style="height:50px" />
                        <div class="caption">
                            <p>@Html.ActionLink("User List", "GetUsers", "Account", null, new { @class = "btn btn-info btn-block" })</p>
                        </div>
                    </div>
                </div>



            </div>
        </div>
    </div>
</div>

